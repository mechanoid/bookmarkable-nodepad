  },
  "devDependencies": {
    "csso-cli": "^3.0.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "fs-extra": "^9.0.1",
    "pug": "^3.0.0",
    "terser": "^5.3.8"
  },
  "type": "module",
  "module": "index.js"
